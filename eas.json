{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "hooks": {
        "prebuild": "npm run diagnose"
      }
    },
    "preview": {
      "distribution": "internal",
      "hooks": {
        "prebuild": "npm run diagnose"
      }
    },
    "production": {
      "autoIncrement": true,
      "cache": {
        "disabled": true
      },
      "hooks": {
        "prebuild": "npm run diagnose"
      }
    }
  },
  "submit": {
    "production": {}
  }
}